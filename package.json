  "private": true,
  "scripts": {
    "android-m1": "source ~/.bash_profile&&react-native run-android",
    "build-m1": "source ~/.bash_profile&&react-native run-android --variant=release",
    "android": "source ~/.bash_profile&&react-native run-android",
    "ios-run": "cd ios && open -a Xcode linkpos.xcworkspace",
    "android-run": "open -b com.google.android.studio android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write **/*.{js,ts,tsx}",
    "lint": "yarn tsc & eslint --ext .ts,.tsx .",
    "schema:codegen": "gqlg --schemaFilePath ./schema/schema.graphql --destDirPath ./__generated__ --depthLimit 5 && prettier --write --loglevel silent __generated__/**/*.gql",
    "codegen": "graphql-codegen --config codegen/codegen.js && yarn schema:codegen",
    "typegen": "graphql-codegen --config codegen/typegen.yml",
    "build": "react-native run-android --variant=release",
    "android:aab-release": "cd android && ./gradlew bundleRelease && cd ..",
    "clean:android": "cd ./android && ./gradlew clean && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "gen-app-key-ios": "appcenter codepush deployment list -a namdq97/Sayang-IOS --displayKeys",
    "newc": "cd src/components && mkdir baseComponent && cp -a base/* baseComponent/",
    "push": "git push origin namdq-uat",
    "pull": "git pull origin develop",
    "port": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:9090 tcp:9090"
  },
